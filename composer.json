{
    "name": "phpunit/phpunit-mink-trait",
    "description": "Trait that provides minimal integration layer for using Mink in PHPUnit test cases",
    "homepage": "https://www.github.com/sebastianbergmann/phpunit-mink-trait",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "phpunit/phpunit": "~5.0",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

